<Window x:Class="Launcher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Launcher"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="99*"/>
            <RowDefinition Height="61*"/>
        </Grid.RowDefinitions>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Height="60" VerticalAlignment="Top" Width="103" Click="button_Click" Grid.Row="0"/>
        <StackPanel Orientation="Vertical" Grid.Row="1">
            <Label Content="Download Progress"/>
            <ProgressBar Margin="5" Height="20"/>
            <Label Content="Install Progress"/>
            <ProgressBar Margin="5" Height="20"/>
        </StackPanel>
    </Grid>
</Window>
